{
    "fileTypes": [],
    "injectionSelector": "L:markup.fenced_code.block.markdown",
    "patterns": [
        {
            "include": "#lhask-code-block"
        }
    ],
    "repository": {
        "haskell-code-block": {
            "begin": "(lhask|lhls)(\\s+[^`~]*)?$",
            "end": "(^|\\G)(?=\\s*[`~]{3,}\\s*$)",
            "patterns": [
                {
                    "begin": "(^|\\G)(\\s*)(.*)",
                    "while": "(^|\\G)(?!\\s*([`~]{3,})\\s*$)",
                    "contentName": "meta.embedded.block.lhask",
                    "patterns": [
                        {
                            "include": "text.tex.latex.haskell"
                        }
                    ]
                }
            ]
        }
    },
    "scopeName": "markdown.lhask.codeblock"
}
